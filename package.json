{
    "name": "zk-proof-of-humanity",
    "description": "Contracts to integrate semaphore with proof of humanity",
    "version": "1.0.0",
    "license": "MIT",
    "private": true,
    "main": "index.js",
    "scripts": {
        "start": "hardhat node & yarn compile && yarn deploy --network localhost",
        "compile": "hardhat compile",
        "download:snark-artifacts": "hardhat run scripts/download-snark-artifacts.ts",
        "deploy": "yarn compile && hardhat deploy",
        "test": "hardhat test",
        "test:report-gas": "REPORT_GAS=true hardhat test",
        "test:coverage": "hardhat coverage",
        "typechain": "hardhat typechain",
        "docgen": "npx hardhat docgen",
        "lint": "solhint 'contracts/**/*.sol'",
        "prettier": "prettier -c .",
        "prettier:fix": "prettier -w"
    },
    "keywords": [
        "javascript",
        "typescript",
        "monorepo",
        "zero-knowledge",
        "semaphore",
        "proof-of-humanity"
    ],
    "devDependencies": {
        "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
        "@nomicfoundation/hardhat-toolbox": "^2.0.1",
        "@nomiclabs/hardhat-ethers": "^2.0.0",
        "@nomiclabs/hardhat-etherscan": "^3.1.5",
        "@semaphore-protocol/group": "3.0.0",
        "@semaphore-protocol/hardhat": "3.0.0",
        "@semaphore-protocol/identity": "3.0.0",
        "@semaphore-protocol/proof": "3.0.0",
        "@typechain/ethers-v5": "^10.1.0",
        "@typechain/hardhat": "^6.1.2",
        "@types/chai": "^4.3.1",
        "@types/download": "^8.0.1",
        "@types/mocha": "^9.1.1",
        "chai": "^4.2.0",
        "circomlibjs": "0.0.8",
        "dotenv": "^16.0.3",
        "download": "^8.0.0",
        "ethers": "^5.4.7",
        "hardhat": "^2.11.0",
        "hardhat-gas-reporter": "^1.0.8",
        "prettier": "^2.8.3",
        "prettier-plugin-solidity": "^1.0.0-beta.19",
        "solhint": "^3.3.6",
        "solhint-plugin-prettier": "^0.0.5",
        "solidity-coverage": "^0.8.1",
        "solidity-docgen": "^0.6.0-beta.34",
        "ts-node": "^10.8.1",
        "typechain": "^8.1.0",
        "typescript": "^4.7.3"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^4.8.1",
        "@semaphore-protocol/contracts": "3.0.0"
    },
    "config": {
        "solidity": {
            "version": "0.8.4"
        },
        "paths": {
            "contracts": "./contracts",
            "tests": "./test",
            "cache": "./cache",
            "build": {
                "snark-artifacts": "./build/snark-artifacts",
                "contracts": "./build/contracts",
                "typechain": "./build/typechain"
            }
        }
    }
}
